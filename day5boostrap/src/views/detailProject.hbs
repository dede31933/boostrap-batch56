<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Personal Web</title>
    <link
      rel="stylesheet"
      href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.4/css/all.min.css"
    />
    <link
      rel="stylesheet"
      href="https://stackpath.bootstrapcdn.com/bootstrap/4.5.2/css/bootstrap.min.css"
    />
    <link rel="stylesheet" href="/src/assests/css/Nav.css" />
    <link rel="stylesheet" href="/src/assests/css/Detail.css" />
  </head>
  <body>
    <!-- nav start -->
    <nav class="navbar navbar-expand-lg navbar-light bg-light">
      <a class="navbar-brand" href="#">
        <img src="/src/assests/image/logggo.png" alt="ini logo" />
      </a>
      <button
        class="navbar-toggler"
        type="button"
        data-toggle="collapse"
        data-target="#navbarNav"
        aria-controls="navbarNav"
        aria-expanded="false"
        aria-label="Toggle navigation"
      >
        <span class="navbar-toggler-icon"></span>
      </button>
      <div class="collapse navbar-collapse" id="navbarNav">
        <ul class="navbar-nav mr-auto">
          <li class="nav-item">
            <a class="nav-link" href="/">Home</a>
          </li>
          <li class="nav-item">
            <a class="nav-link" href="/AddmyProject">Add Project</a>
          </li>
          <li class="nav-item">
            <a class="nav-link" href="/testimonial">Testimonial</a>
          </li>
        </ul>
        <button type="button" class="btn btn-secondary">
          <a
            href="/contact"
            class="contact-link text-decoration-none text-wrap"
            style="width: 6rem;"
          >Contact Me</a>
        </button>
      </div>
    </nav>
    <!-- nav end -->

    <!-- content start -->
    <div class="container mt-5">
      <h1 class="mb-4">Dumbways Web App</h1>
      <div class="row">
        <div class="col-md-6 mb-4">
          <div class="project-image">
            <img
              src="/src/assests/image/Sukuna-1-1.jpg"
              alt="Project Image"
              class="img-fluid rounded"
            />
          </div>
        </div>
        <!-- HTML untuk project-info -->
        <div class="col-md-6">
          <div class="project-info">
            <div class="project-duration mb-3">
              <p><strong>Duration</strong></p>
              <p><i class="fas fa-calendar-alt"></i>
                 22 Agustus 2024 - 11 Desember 2024</p>
               <p><i class="far fa-clock"></i> {{duration project.start_date project.end_date}}</p>
              </p>
            </div>
            <div class="project-technologies">
              <p><strong>Technologies</strong></p>
              <div class="technologies">
                {{#each project.technologies}}
                  {{#ifCond this "React Js"}}
                    <div class="technology"><i class="fab fa-react"></i>
                      React Js</div>
                  {{/ifCond}}
                  {{#ifCond this "Node Js"}}
                    <div class="technology"><i class="fab fa-node-js"></i>
                      Node Js</div>
                  {{/ifCond}}
                  {{#ifCond this "TypeScript"}}
                    <div class="technology"><svg
                        xmlns="http://www.w3.org/2000/svg"
                        x="0px"
                        y="0px"
                        width="25"
                        height="35"
                        viewBox="0 0 50 50"
                        class="custom-size"
                      >
                        <path
                          d="M 5 4 A 1.0001 1.0001 0 0 0 4 5 L 4 45 A 1.0001 1.0001 0 0 0 5 46 L 45 46 A 1.0001 1.0001 0 0 0 46 45 L 46 5 A 1.0001 1.0001 0 0 0 45 4 L 5 4 z M 6 6 L 44 6 L 44 44 L 6 44 L 6 6 z M 15 23 L 15 26.445312 L 20 26.445312 L 20 42 L 24 42 L 24 26.445312 L 29 26.445312 L 29 23 L 15 23 z M 36.691406 23.009766 C 33.576782 22.997369 30.017578 23.941219 30.017578 28.324219 C 30.017578 34.054219 37.738281 34.055625 37.738281 36.640625 C 37.738281 36.885625 37.842187 38.666016 35.117188 38.666016 C 32.392187 38.666016 30.121094 36.953125 30.121094 36.953125 L 30.121094 41.111328 C 30.121094 41.111328 42.001953 44.954062 42.001953 36.289062 C 42.000953 30.664063 34.208984 30.945391 34.208984 28.150391 C 34.208984 27.067391 34.978375 26.054687 37.109375 26.054688 C 39.240375 26.054688 41.126953 27.3125 41.126953 27.3125 L 41.267578 23.607422 C 41.267578 23.607422 39.113892 23.019408 36.691406 23.009766 z"
                        ></path>
                      </svg>
                      TypeScript</div>
                  {{/ifCond}}
                  {{#ifCond this "Next Js"}}
                    <div class="technology"><svg
                        xmlns="http://www.w3.org/2000/svg"
                        x="0px"
                        y="0px"
                        width="25"
                        height="35"
                        viewBox="0 0 48 48"
                        class="custom-size"
                      >
                        <path
                          fill="#212121"
                          d="M18.974,31.5c0,0.828-0.671,1.5-1.5,1.5s-1.5-0.672-1.5-1.5v-14c0-0.653,0.423-1.231,1.045-1.43 c0.625-0.198,1.302,0.03,1.679,0.563l16.777,23.704C40.617,36.709,44,30.735,44,24c0-11-9-20-20-20S4,13,4,24s9,20,20,20 c3.192,0,6.206-0.777,8.89-2.122L18.974,22.216V31.5z M28.974,16.5c0-0.828,0.671-1.5,1.5-1.5s1.5,0.672,1.5,1.5v13.84l-3-4.227 V16.5z"
                        ></path>
                      </svg>Next Js</div>
                  {{/ifCond}}
                {{/each}}
              </div>
            </div>
          </div>
        </div>
      </div>
      <div class="project-description mt-4">
        <p>{{project.descriptions}}</p>
      </div>
    </div>
    <!-- content end -->
  </body>
</html>
              {{!--  --}}